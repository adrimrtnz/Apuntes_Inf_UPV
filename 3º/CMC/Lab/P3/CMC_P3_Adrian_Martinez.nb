                    RowBox[{"pos2", "->", "Nothing"}]}], "}"}]}], "]"}]}], 
                  ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
               "\[IndentingNewLine]", 
               RowBox[{"j", " ", "+=", " ", "randomRule"}], ";"}]}], 
             "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
          "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
