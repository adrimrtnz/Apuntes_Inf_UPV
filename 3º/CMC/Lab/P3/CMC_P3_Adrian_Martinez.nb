Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"CelulaDividor", "[", 
    RowBox[{"num_Integer", ",", " ", "k_Integer"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "config", ",", "i", ",", " ", "posA", ",", " ", "posC", ",", "posE", 
       ",", " ", "random", ",", " ", "pos1", ",", " ", "pos2"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"config", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", "}"}], ",", 
         RowBox[{"{", "}"}]}], "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "<=", "num"}], ",", 
        RowBox[{"i", "++"}], ",", " ", 
        RowBox[{"AppendTo", "[", 
         RowBox[{
          RowBox[{"config", "[", 
           RowBox[{"[", "1", "]"}], "]"}], ",", "a"}], "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "<=", "k"}], ",", 
        RowBox[{"i", "++"}], ",", " ", 
        RowBox[{"AppendTo", "[", 
         RowBox[{
          RowBox[{"config", "[", 
           RowBox[{"[", "1", "]"}], "]"}], ",", "c"}], "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"AppendTo", "[", 
       RowBox[{
        RowBox[{"config", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", "d"}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
