        RowBox[{
         RowBox[{"Length", "[", 
          RowBox[{"config", "[", 
           RowBox[{"[", 
            RowBox[{"-", "1"}], "]"}], "]"}], "]"}], " ", "==", " ", "0"}], 
        ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Print", "[", "\"\<Hola\>\"", "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"posA", "=", 
          RowBox[{"Position", "[", 
           RowBox[{"config", ",", "a"}], "]"}]}], ";", "\[IndentingNewLine]", 
