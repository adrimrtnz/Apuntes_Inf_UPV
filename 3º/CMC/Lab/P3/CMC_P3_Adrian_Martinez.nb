         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"posA", " ", "!=", 
             RowBox[{"{", "}"}]}], " ", "&&", " ", 
            RowBox[{"posC", " ", "!=", 
             RowBox[{"{", "}"}]}], " ", "&&", " ", 
            RowBox[{"posE", " ", "!=", 
             RowBox[{"{", "}"}]}]}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"random", " ", "=", " ", 
             RowBox[{"RandomInteger", "[", 
              RowBox[{"{", 
               RowBox[{"1", ",", "2"}], "}"}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"Print", "[", 
             RowBox[{"\"\<Regla: \>\"", ",", " ", "random"}], "]"}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"random", "==", "1"}], ",", "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"Length", "[", "posC", "]"}], " ", ">=", " ", "k"}], 
