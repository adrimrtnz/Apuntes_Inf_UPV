               RowBox[{"(*", "Else", "*)"}], "\[IndentingNewLine]", 
               RowBox[{"If", "[", 
                RowBox[{
                 RowBox[{
                  RowBox[{"Length", "[", "posE", "]"}], " ", ">=", " ", "k"}],
