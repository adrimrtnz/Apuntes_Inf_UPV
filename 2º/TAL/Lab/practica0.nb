Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Contador", "[", 
    RowBox[{"pal_List", ",", " ", "element_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "counter", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"counter", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "0"}], ",", " ", 
        RowBox[{"i", "<=", 
         RowBox[{"Length", "[", "pal", "]"}]}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"pal", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "\[Equal]", "element"}], ",", 
          RowBox[{"counter", "++"}]}], "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "counter", "]"}]}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.841306892976108*^9, 3.8413071220792003`*^9}, {
