   RowBox[{"Segmentos", "[", 
    RowBox[{"x_", ",", "k_Integer"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "lista", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"lista", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", " ", 
        RowBox[{"i", "\[LessEqual]", 
         RowBox[{
          RowBox[{"Length", "[", "x", "]"}], "-", "k", "+", "1"}]}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"lista", ",", " ", 
          RowBox[{"x", "[", 
           RowBox[{"[", 
            RowBox[{"i", ";;", 
